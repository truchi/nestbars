{{!-- TypeORM --}}
import {
Entity,
PrimaryGeneratedColumn,
{{#if entity.fields.length}} Column, {{/if}}
}
from 'typeorm'
{{!-- Nest GraphQL --}}
import {
ObjectType,
{{#if entity.fields.length}} Field, {{/if}}
{{#if (hasInts   entity)}} Int, {{/if}}
{{#if (hasFloats entity)}} Float, {{/if}}
{{#if (hasEnums  entity)}} registerEnumType, {{/if}}
}
from '@nestjs/graphql'
{{!-- Dependencies --}}
{{#each (call entity 'dependencies') as |dependency|}}
  import { {{dependency.name}} }
  from '{{relative (dirname ../entity.dest) (dirname dependency.dest)}}/{{stem dependency.dest}}'
{{/each}}
