{{!-- TypeORM --}}
import {
Entity,
PrimaryGeneratedColumn,
{{#if entity.fields.length}} Column, {{/if}}
{{#if (length (call entity 'fieldsByType' 'created'))}} CreateDateColumn, {{/if}}
{{#if (length (call entity 'fieldsByType' 'updated'))}} UpdateDateColumn, {{/if}}
{{#if (length (call entity 'fieldsByType' 'version'))}} VersionColumn, {{/if}}
}
from 'typeorm'
{{!-- Nest GraphQL --}}
import {
ObjectType,
{{#if entity.fields.length}} Field, {{/if}}
{{#if (length (call entity 'fieldsByType' 'int'))}} Int, {{/if}}
{{#if (length (call entity 'fieldsByType' 'float'))}} Float, {{/if}}
{{#if (length (call entity 'fieldsByType' 'enum' 'set'))}} registerEnumType, {{/if}}
}
from '@nestjs/graphql'
{{!-- Dependencies --}}
{{#each (call entity 'dependencies') as |dependency|}}
  import { {{dependency.name}} }
  from '{{relative (dirname ../entity.dest) (dirname dependency.dest)}}/{{stem dependency.dest}}'
{{/each}}
