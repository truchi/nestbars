import { ArgsType, Field } from '@nestjs/graphql'
import {
{{#each ($get 'entity.dataEnums') as |enum|}}
  {{enum}},
{{/each}}
}
from '{{relativeImport ($get 'entity.createDtoPath') ($get 'entity.entityPath')}}'
{{#each ($get 'entity.dataRelations') as |relation|}}
  import { {{relation.name}} }
  from '{{relativeImport
    ($get 'entity.createDtoPath')
    (lookup (call relation 'data') 'entityPath')
  }}'
{{/each}}
