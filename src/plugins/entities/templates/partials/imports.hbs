import {
{{#each ($get 'entity.dbImports') as |name|}}
  {{name}},
{{/each}}
} from 'typeorm'
import {
{{#each ($get 'entity.gqlImports') as |name|}}
  {{name}},
{{/each}}
} from '@nestjs/graphql'
{{#each entity.relations as |relation|}}
  import { {{relation.name}} }
  from '{{relativeImport
    ($get 'entity.path')
    (lookup (call relation 'data') 'path')
  }}'
{{/each}}
