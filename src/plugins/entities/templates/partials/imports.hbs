import {
{{#each ($get 'entity.dbImports') as |name|}}
  {{name}},
{{/each}}
} from 'typeorm'
import {
{{#each ($get 'entity.gqlImports') as |name|}}
  {{name}},
{{/each}}
} from '@nestjs/graphql'
{{#each entity.dependencies as |dependency|}}
  import { {{dependency.name}} }
  from '{{relativeImport
    ($get 'entity.path')
    (lookup (call dependency 'data') 'path')
  }}'
{{/each}}
