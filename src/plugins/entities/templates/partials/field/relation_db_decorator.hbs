@
{{#switch field.type.type}}
  {{#case 'one-to-one'}}OneToOne{{/case}}
  {{#case 'one-to-many'}}OneToMany{{/case}}
  {{#case 'many-to-one'}}ManyToOne{{/case}}
  {{#case 'many-to-many'}}ManyToMany{{/case}}
{{/switch}}
(
() => {{call field 'dbType'}},
{{!-- (
{{uncapitalize (call field 'dbType')}}: {{call field 'dbType'}}
) => {{uncapitalize (call field 'dbType')}}.{{field.withField}} --}}
)
{{#switch field.type.type}}
  {{#case 'one-to-one' 'many-to-one'}}
    {{#if field.options.options.joinColumn}}
      @JoinColumn(
      {{#if (isObject field.options.options.joinColumn)}}
        {{stringify field.options.options.joinColumn trap='{}'}}
      {{/if}}
      )
    {{/if}}
  {{/case}}
  {{#case 'many-to-many'}}
    {{#if field.options.options.joinTable}}
      @JoinTable(
      {{#if (isObject field.options.options.joinTable)}}
        {{stringify field.options.options.joinTable trap='{}'}}
      {{/if}}
      )
    {{/if}}
  {{/case}}
{{/switch}}
