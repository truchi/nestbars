{{#each (enums) as |enum|}}
  export enum {{enum.options.name}} {
  {{#each enum.options.values as |value|}}
    {{value}} = '{{value}}',
  {{/each}}
  }

  registerEnumType({{enum.options.name}}, {
  name: '{{enum.options.name}}',
  {{#if enum.options.description}}
    description: '{{enum.options.description}}',
  {{/if}}
  })

{{/each}}
